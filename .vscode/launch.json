  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Producer",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/dist/example/producer.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Consumer",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/dist/example/consumer.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Tests",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/node_modules/jasmine/bin/jasmine.js",
      "args": [
        "JASMINE_CONFIG_PATH=src/test/jasmine.json"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}