{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Silent Producer",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/packages/example/src/silent-producer.ts",
      "outFiles": ["${workspaceFolder}/**/*"],
      "console": "integratedTerminal",
      "env": {
        "SCHEMA_FOLDER": "${workspaceFolder}/packages/example/src/schemas",
        "PARTITION_SPECIFICATION": "system_heartbeat:1:600000, simulation_time_mgmt:1:60000000, system_logging"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Producer",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/packages/example/src/producer.ts",
      "outFiles": ["${workspaceFolder}/**/*"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch SSL Producer",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/packages/example/src/producer-secure.ts",
      "outFiles": ["${workspaceFolder}/**/*"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Consumer",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/packages/example/src/consumer.ts",
      "outFiles": ["${workspaceFolder}/**/*"],
      "console": "integratedTerminal" //,
      // "env": {
      //   "DEBUG": "kafka-node:*"
      // }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Basic Consumer",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/packages/example/src/consumer-basic-registration.ts",
      "outFiles": ["${workspaceFolder}/**/*"],
      "console": "integratedTerminal" //,
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Time Consumer",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/src/example/consumer-time.ts",
      "outFiles": ["${workspaceFolder}/**/*"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch SSL  Consumer",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/src/example/consumer-secure.ts",
      "outFiles": ["${workspaceFolder}/**/*"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Tests",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/node_modules/jasmine/bin/jasmine.js",
      "args": ["JASMINE_CONFIG_PATH=src/test/jasmine.json"],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
